class Solution {
Input: nums = [1,2,0]
Output: 3
  fun setZeroes(m: Array<IntArray>): Unit {
    var arr = ArrayList<Pair<Int, Int>>()
    for (i in m.indices) {
      for (j in m[0].indices) {
        if (m[i][j] == 0) arr.add(i to j)
      }
    }
    val cpy = m.clone()
    for (i in arr) {
      var k = i.first
      var l = i.second
      var i1 = 0
      while (i1 < m.size) cpy[i1++][l] = 0
      var j1 = 0
      while (j1 < m[0].size) cpy[k][j1++] = 0
    }
    println(cpy)
    for (i in m.indices) {
      for (j in m[0].indices) {
        m[i][j] = cpy[i][j]
      }
    }
  }
}