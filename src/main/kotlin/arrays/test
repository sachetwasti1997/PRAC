class Solution {
intervals = [[0,2],[3,4],[5,7]], toBeRemoved = [1,6]
Output: [[0,1],[6,7]]
  fun removeInterval(intervals: Array<IntArray>, r: IntArray): List<List<Int>> {
      val res = ArrayList<List<Int>>()
      for (i in intervals) {
        if (i[0] < r[0]) res.add(listOf(r[0].coerceAtMost(i[0]), i[1]))
        if (i[1] > r[1]) res.add(listOf(i[0], r[1].coerceAtLeast(i[1]))
      }
      return res
    }
}