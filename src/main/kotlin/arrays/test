class Solution {
Input: nums = [1,2,2,4]
Output: [2,3]
  fun findErrorNums(nums: IntArray): IntArray {
    val mp = HashMap<Int, Int>()
    for (i in nums) mp[i] = mp.getOrPut(i){0}+1
    val res = IntArray(2)
    var i = 0
    for ((k, v) in mp)
      if (v == 2)
        res[i++] = k
    for (i1 in 1 .. nums.size)
      if (i1 !in mp) res[i++] = i1
    return res
  }
}